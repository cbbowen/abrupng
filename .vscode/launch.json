				"args": [
					"build",
					"--bin=abrupng",
					"--package=abrupng"
				],
				"filter": {
					"name": "abrupng",
					"kind": "bin"
				}
			},
			"args": [
				"C:/Users/chris/Downloads/mike_nash_charcoal/PAPER OVERLAY (Styles).asl",
				"-o",
				"C:/Users/chris/Downloads/mike_nash_charcoal/bleh"
			],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'abrupng'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=abrupng",
					"--package=abrupng"
				],
				"filter": {
					"name": "abrupng",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
